<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:mapsui="clr-namespace:Mapsui.UI.Maui;assembly=Mapsui.UI.Maui"
             x:Class="MAUI1.User.Dispatcher.Orders.OrderHandlerPage"
             Title="OrderHandlerPage">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="3*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <mapsui:MapView x:Name="mapView"></mapsui:MapView>
        <Label Grid.Row="1" Text="{Binding PinList[0].User.Name}" HorizontalOptions="Center" VerticalOptions="Center"></Label>
    </Grid>
</ContentPage>